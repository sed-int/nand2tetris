/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    And(a=a, b=sel, out=x);
    And(a=nsel, b=b, out=y);
    Or(a=x, b=y, out=out);
}